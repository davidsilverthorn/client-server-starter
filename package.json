{
    "engines": {
        "npm": ">=7.19.0"
    },
    "name": "client-server-starter",
    "version": "0.0.1",
    "scripts": {
        "build": "cross-env NODE_ENV=production npm run build -w client",
        "dev": "cross-env NODE_ENV=development run-p dev:*",
        "dev:build": "npm run dev -w client",
        "dev:serve": "npm run serve",
        "postinstall": "npm run build",
        "serve": "npm run start -w server"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/davidsilverthorn/client-server-starter.git"
    },
    "author": "David Silverthorn",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/davidsilverthorn/client-server-starter/issues"
    },
    "homepage": "https://github.com/davidsilverthorn/client-server-starter#readme",
    "workspaces": [
        "packages/*"
    ],
    "dependencies": {
        "cross-env": "^7.0.3",
        "npm-run-all": "^4.1.5"
    }
}
